AProgram.       Program ::= [Term] ;

Var.            Term ::= Variable ;
IntConst.       Term ::= Integer ;
DoubleConst.    Term ::= Double ;
-- Binder.         Term ::= "Î»" Variable "." Term ;
Binder.         Term ::= "lambda" Variable "." Term ;
Application.    Term ::= "(" Term Term ")" ;
Plus.           Term ::= Term "+" Term  ;
Minus.          Term ::= Term "-" Term  ;
_.              Term ::= "(" Term ")";
separator Term ";";

Identifier.     Variable ::= Ident;
